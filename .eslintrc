{
  "env": {
    "browser": true
  },
  "extends": [
    "standard",
    "plugin:vue/recommended"
  ],
  "rules": {
    "vue/require-default-prop": "off",
    "no-console": ["error", { "allow": ["warn", "error"] }]
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}